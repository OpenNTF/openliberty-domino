<?xml version="1.0" encoding="UTF-8"?>
<form name="Configuration" publicaccess="false" renderpassthrough="true" xmlns="http://www.lotus.com/dxl">
  <code event="windowtitle">
    <formula>"Server"</formula>
  </code>
  <actionbar bgcolor="#d4d0c8" bordercolor="black">
    <actionbuttonstyle bgcolor="#d4d0c8"/>
    <font color="system"/>
    <border style="solid" width="0px 0px 1px"/>
    <action align="right" hide="edit previewedit" icon="4" title="Delete">
      <code event="click">
        <formula>@Command([EditClear])</formula>
      </code>
    </action>
    <action hide="edit previewedit" icon="5" title="Edit">
      <code event="click">
        <formula>@Command([EditDocument])</formula>
      </code>
      <code event="hidewhen">
        <formula>@ToNumber(@UserAccess(@DbName; [AccessLevel])) &lt; 4 &amp; !(@Name([Abbreviate]; @UserName)=@Author)</formula>
      </code>
    </action>
    <action hide="preview read" title="Save">
      <code event="click">
        <formula>@If(
	@ClientType="Web" | @IsValid; @Do(
		@Command([FileSave]);
		@Command([EditDocument])
	);
	""
)</formula>
      </code>
    </action>
    <action hide="preview read" title="Save and Close">
      <code event="click">
        <formula>@If(
	@ClientType="Web" | @IsValid; @Do(
		@Command([FileSave]);
		@Command([FileCloseWindow])
	);
	""
)</formula>
      </code>
    </action>
  </actionbar>
  <body>
    <richtext>
      <pardef id="1"/>
      <par def="1"/>
      <par def="1">
        <run>
          <font size="18pt" style="bold"/>Configuration</run>
      </par>
      <par def="1"/>
      <pardef id="2" leftmargin="1in" spaceafter="1.5">
        <parstyle alldocuments="true" incyclekey="true" name="Headline" username="Jesse Gallagher-">
          <font color="purple" name="sans-serif" size="12pt" style="bold"/>
        </parstyle>
      </pardef>
      <par def="2">
        <run>
          <font color="purple" size="12pt" style="bold"/>Liberty Runtime</run>
      </par>
      <par def="1"/>
      <table altbgcolor="white" bgcolor="#efefef" colorstyle="left"
        columnspacing="0.0500in" refwidth="8.7292in"
        rowspacing="0.0500in" widthtype="fitmargins">
        <tablecolumn width="2in"/>
        <tablecolumn width="100%"/>
        <tablerow>
          <tablecell borderwidth="0px">
            <pardef id="4" keeptogether="true" keepwithnext="true"/>
            <par def="4">Base Directory</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <pardef id="5" keeptogether="true" keepwithnext="true"/>
            <par def="5">
              <field kind="editable" name="BaseDirectory" type="text"/>
              <compositedata afterbegincount="3" aftercontainercount="1"
                afterparcount="7" containertype="65418" nexttype="222"
                prevtype="65418" type="98">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
            </par>
            <pardef hide="read print preview" id="6" keeptogether="true" keepwithnext="true"/>
            <par def="6">
              <run>
                <font color="gray" size="8pt"/>Default: (program directory)/wlp</run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Version</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="5">
              <field kind="editable" name="Version" type="keyword">
                <keywords allownew="true" ui="dialoglist">
                  <textlist>
                    <text>20.0.0.5</text>
                  </textlist>
                </keywords>
                <code event="defaultvalue">
                  <formula>"19.0.0.7"</formula>
                </code>
              </field>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Artifact <run>
                <font color="gray" size="8pt"/>(advanced)</run>
            </par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="5">
              <field kind="editable" name="Artfiact" type="text"/>
              <compositedata afterbegincount="3" aftercontainercount="1"
                afterparcount="6" containertype="65418" nexttype="222"
                prevtype="65418" type="98">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
            </par>
            <par def="6">
              <run>
                <font color="gray" size="8pt"/>Default: io.openliberty:openliberty-runtime</run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Maven Repo <run>
                <font color="gray" size="8pt"/>(advanced)</run>
            </par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="5">
              <field kind="editable" name="MavenRepo" type="text"/>
              <compositedata afterbegincount="3" aftercontainercount="1"
                afterparcount="6" containertype="65418" nexttype="222"
                prevtype="65418" type="98">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
            </par>
            <par def="6">
              <run>
                <font color="gray" size="8pt"/>Default: https://repo.maven.apache.org/maven2/</run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Java Runtime</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <pardef id="7" keeptogether="true" keepwithnext="true" linespacing="1.5"/>
            <par def="7">
              <field kind="editable" lookupaddressonrefresh="false"
                lookupeachchar="false" name="JavaVersion" type="keyword">
                <keywords allownew="true" columns="1"
                  recalconchange="true" ui="dialoglist">
                  <textlist>
                    <text>Domino JVM|Domino</text>
                    <text>1.8 (LTS)|1.8</text>
                    <text>11 (LTS)|11</text>
                    <text>14</text>
                  </textlist>
                </keywords>
                <code event="defaultvalue">
                  <formula>"Domino"</formula>
                </code>
              </field>
            </par>
            <pardef id="8" keeptogether="true" keepwithnext="true" linespacing="1.5">
              <code event="hidewhen">
                <formula>JavaVersion="":"Domino"</formula>
              </code>
            </pardef>
            <par def="8">
              <field borderstyle="none" kind="editable"
                lookupaddressonrefresh="false" lookupeachchar="false"
                name="JavaJVM" type="keyword">
                <keywords columns="2" helperbutton="false" ui="radiobutton">
                  <textlist>
                    <text>HotSpot</text>
                    <text>OpenJ9</text>
                  </textlist>
                </keywords>
                <code event="defaultvalue">
                  <formula>"HotSpot"</formula>
                </code>
              </field>
            </par>
            <par def="8">
              <run>
                <font color="gray" size="8pt"/>More Information: </run>
              <urllink href="https://adoptopenjdk.net/" showborder="false">
                <run>
                  <font color="gray" size="8pt"/>https://adoptopenjdk.net</run>
              </urllink>
              <run>
                <font color="gray" size="8pt"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
      </table>
      <pardef firstlineleftmargin="1in" id="3"/>
      <par def="3"/>
      <par def="2">
        <run>
          <font color="purple" size="12pt" style="bold"/>Reverse Proxy</run>
      </par>
      <par def="1"/>
      <table altbgcolor="white" bgcolor="#efefef" colorstyle="left"
        columnspacing="0.0500in" refwidth="8.7292in"
        rowspacing="0.0500in" widthtype="fitmargins">
        <tablecolumn width="2in"/>
        <tablecolumn width="100%"/>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Enable Reverse Proxy</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="6">
              <run>
                <font color="gray" size="8pt"/>
              </run>
              <field borderstyle="none" kind="editable"
                lookupaddressonrefresh="false" lookupeachchar="false"
                name="ReverseProxyEnable" type="keyword">
                <keywords columns="2" helperbutton="false" ui="radiobutton">
                  <textlist>
                    <text>Yes|Y</text>
                    <text>No|N</text>
                  </textlist>
                </keywords>
              </field>
              <run>
                <font color="gray" size="8pt"/>
              </run>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Bind to Host</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="5">
              <field fieldhint="*" kind="editable"
                name="ReverseProxyHostName" type="text"/>
              <compositedata afterbegincount="4" aftercontainercount="2"
                afterparcount="7" containertype="65418" nexttype="222"
                prevtype="65513" type="98">
Yg4BAIQAAAAAAAAAAAA=
</compositedata>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Port</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="5">
              <field kind="editable" name="ReverseProxyPort" type="number">
                <numberformat bytes="false" digits="2" format="general"
                  parens="false" percent="false" punctuated="false"/>
                <code event="defaultvalue">
                  <formula>8080</formula>
                </code>
              </field>
            </par>
          </tablecell>
        </tablerow>
        <tablerow>
          <tablecell borderwidth="0px">
            <par def="4">Use Connector Headers for Domino</par>
          </tablecell>
          <tablecell borderwidth="0px">
            <par def="6">
              <field borderstyle="none" kind="editable"
                lookupaddressonrefresh="false" lookupeachchar="false"
                name="ReverseProxyConnectorHeaders" type="keyword">
                <keywords columns="2" helperbutton="false" ui="radiobutton">
                  <textlist>
                    <text>Yes|Y</text>
                    <text>No|N</text>
                  </textlist>
                </keywords>
              </field>
            </par>
          </tablecell>
        </tablerow>
      </table>
      <par def="3"/>
      <par def="3"/>
      <subformref name="Footer"/>
      <pardef align="right" id="9">
        <code event="hidewhen">
          <formula>@IsNewDoc</formula>
        </code>
      </pardef>
      <par def="9"/>
    </richtext>
  </body>
